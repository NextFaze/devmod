{
  "name": "devmod",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "ng build core --prod && ng build interface --prod && ng build --prod && tsc -p projects/schematic/tsconfig.json",
    "test:all": "ng test core --code-coverage --watch=false --browsers ChromeHeadless && ng test interface --code-coverage --watch=false --browsers ChromeHeadless && istanbul cover ./node_modules/.bin/jasmine projects/schematic/**/*_spec.js --dir=coverage/schematic",
    "pretest": "npm run build:all",
    "test": "npm run test:all",
    "posttest": "npm run lint",
    "coveralls": "./node_modules/coveralls/bin/coveralls.js < coverage/core/lcov.info && ./node_modules/coveralls/bin/coveralls.js < coverage/interface/lcov.info && ./node_modules/coveralls/bin/coveralls.js < coverage/schematic/lcov.info",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "angular2-draggable": "^1.4.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cdk": "^6.0.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@schematics/angular": "^0.6.5",
    "@types/faker": "^4.1.2",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^1.7.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "coveralls": "^3.0.1",
    "faker": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
