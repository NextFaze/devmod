{
  "name": "devmod",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "ng build core --prod && ng build interface --prod && ng build --prod && tsc -p projects/schematic/tsconfig.json",
    "test:all": "ng test core --code-coverage --watch=false --browsers ChromeHeadless && ng test interface --code-coverage --watch=false --browsers ChromeHeadless && istanbul cover -i projects/schematic/**/*_spec.js --dir=coverage/schematic ./node_modules/.bin/jasmine",
    "pretest": "npm run build:all",
    "test": "npm run test:all",
    "posttest": "npm run lint",
    "coveralls": "cat coverage/core/lcov.info coverage/interface/lcov.info coverage/schematic/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "angular2-draggable": "^2.1.7",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.4",
    "@angular-devkit/build-ng-packagr": "~0.10.4",
    "@angular/cdk": "^7.0.0",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@schematics/angular": "^7.0.4",
    "@types/faker": "^4.1.2",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^3.0.0",
    "@types/node": "~10.12.2",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.1",
    "faker": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": "^0.33.1",
    "tslib": "^1.7.1",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
